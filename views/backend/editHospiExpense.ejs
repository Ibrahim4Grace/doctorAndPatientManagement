<!DOCTYPE html>
<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>Korex Hospital</title>
    <!-- base:css -->
    <link rel="stylesheet" href="../../font/typicons.css">
    <link rel="stylesheet" href="../../css/vendor.bundle.base.css">
    <link rel="stylesheet" href="../../vertical-layout-light/style.css">
    <!-- endinject -->
    <link rel="icon" href="../../img/Createe.jpg">
  </head>
  <body>
    <div class="container-scroller">
      <!-- partial:partials/_navbar.html -->
      <nav class="navbar col-lg-12 col-12 p-0 fixed-top d-flex flex-row">

        <div class="text-center navbar-brand-wrapper d-flex align-items-center justify-content-center">
          <a class="navbar-brand brand-logo" href="/backend/dashboard"><img src="../../img/korexlogo.jpg" alt="logo"/></a>
          <a class="navbar-brand brand-logo-mini" href="/backend/dashboard"><img src="../../img/korexlogo.jpg" alt="logo"/></a>
          <button class="navbar-toggler navbar-toggler align-self-center d-none d-lg-flex" type="button" data-toggle="minimize">
            <span class="typcn typcn-th-menu"></span>
          </button>
        </div>

        <%- include ('../partials/adminNav'); -%>
      </nav>
      <!-- partial -->
      <div class="container-fluid page-body-wrapper">
        <!-- partial:partials/_settings-panel.html -->
        <div class="theme-setting-wrapper">
          <div id="settings-trigger"><i class="typcn typcn-cog-outline"></i></div>
          <div id="theme-settings" class="settings-panel">
            <i class="settings-close typcn typcn-delete-outline"></i>
            <p class="settings-heading">SIDEBAR SKINS</p>
            <div class="sidebar-bg-options" id="sidebar-light-theme">
              <div class="img-ss rounded-circle bg-light border mr-3"></div>
              Light
            </div>
            <div class="sidebar-bg-options selected" id="sidebar-dark-theme">
              <div class="img-ss rounded-circle bg-dark border mr-3"></div>
              Dark
            </div>
            <p class="settings-heading mt-2">HEADER SKINS</p>
            <div class="color-tiles mx-0 px-4">
              <div class="tiles success"></div>
              <div class="tiles warning"></div>
              <div class="tiles danger"></div>
              <div class="tiles primary"></div>
              <div class="tiles info"></div>
              <div class="tiles dark"></div>
              <div class="tiles default border"></div>
            </div>
          </div>
        </div>
        <!-- partial -->
        <!-- partial:partials/_sidebar.html -->
        <%- include('../partials/adminSidebar'); -%>

        <!-- partial -->
        <div class="main-panel">
          <div class="content-wrapper">

              
                <%- include (`../partials/messages`); -%>

                <div class="col-12 grid-margin stretch-card">
                    <div class="card">
                         <div class="card-body">
                           <h4 class="card-title">Edit Expenses </h4>
                           <p class="card-description">
                            Edit Expenses
                           </p>
                           <form class="forms-sample" action="/backend/editHospiExpensePost/<%- hospitalExpenses._id %>" method="post">

                            <div class="form-group">
                              <label for="exampleInputName1">Expense Type</label>
                              <input type="text" class="form-control" name="expenseType"
                              value="<%- hospitalExpenses.expenseType %>"/>
                            </div>

                            <div class="form-group">
                              <label for="exampleInputEmail3">Expense Date</label>
                              <input type="date" class="form-control" name="expenseDate" 
                              value="<%- hospitalExpenses.expenseDate %>"/>
                            </div>

                            <div class="form-group">
                               <label for="exampleInputEmail3">Expense Amount</label>
                               <input type="text" class="form-control" name="expenseAmount" 
                               value="<%- hospitalExpenses.expenseAmount %>"/>
                             </div>

                             <div class="form-group">
                               <label for="exampleInputEmail3">Expense Vendor/Supplier</label>
                               <input type="text" class="form-control" name="expenseVendorSupplier" 
                               value="<%- hospitalExpenses.expenseVendorSupplier %>"/>
                             </div>

                             <div class="form-group">
                               <label for="exampleInputCity1">Expense Category/Department</label>
                               <input type="text" class="form-control" name="expenseCategoryDepartment"
                               value="<%- hospitalExpenses.expenseCategoryDepartment %>"/>
                             </div>

                             <div class="form-group">
                               <label for="exampleInputCity1">Expense Authorized By</label>
                               <input type="text" class="form-control" name="expenseAuthorizedBy"
                               value="<%- hospitalExpenses.expenseAuthorizedBy %>"/>
                             </div>

                             <div class="form-group">
                                <% let paymentMethod=["Cash", "Cheque","Bank Transfer" ] %>
                                <label for="exampleSelectGender">Selected Expense Payment Method</label>
                                <select class="form-control"  name="paymentMethod">
                                    <% paymentMethod.forEach((paymentMethod) => { %>
                                      <option value="<%- paymentMethod %>"<% if (hospitalExpenses.paymentMethod === paymentMethod) { %> selected<% } %>><%- paymentMethod %></option>
                                    <% }) %>
                                </select>
                            </div>

                            <div class="form-group">
                                <% let paymentStatus=["Complete", "Pending" ] %>
                                <label for="exampleSelectGender">Selected Expense Payment Method</label>
                                <select class="form-control"  name="paymentStatus">
                                    <% paymentStatus.forEach((paymentStatus) => { %>
                                      <option value="<%- paymentStatus %>"<% if (hospitalExpenses.paymentStatus === paymentStatus) { %> selected<% } %>><%- paymentStatus %></option>
                                    <% }) %>
                                </select>
                            </div>

                             
                            <button type="submit" class="btn btn-primary mr-2">Submit</button>
                            <button class="btn btn-light">Cancel</button>
                          </form>
                         </div>
                    </div>
                </div>


                  
          
               
  
              
            

        
          </div>
          <%- include (`../partials/adminFooter`); -%>
   
        </div>
    
      </div>
    
    </div>
 

  
  </body>
</html>